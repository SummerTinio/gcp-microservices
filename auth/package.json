{
  "name": "auth",
  "version": "1.0.0",
  "description": "authentication microservice",
  "main": "express-server.js",
  "scripts": {
    "start": "nodemon --legacy-watch src/express-server.ts",
    "start:prod": "nodemon --legacy-watch src/express-server.ts",
    "test": "jest --watchAll --no-cache --detectOpenHandles --maxWorkers 4"
  },
  "jest": {
    "preset": "ts-jest",
    "globalSetup": "<rootDir>/src/test/globalSetup.ts",
    "globalTeardown": "<rootDir>/src/test/globalTeardown.ts",
    "testEnvironment": "node",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "setupFilesAfterEnv": [
      "./src/test/connectionSetup.ts"
    ]
  },
  "baseUrl": ".",
  "paths": {
    "*": [
      "*",
      "src/*"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cookie-session": "^2.0.42",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.3",
    "@types/node": "^15.12.5",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "eslint": "^7.30.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.0.6",
    "mongodb-memory-server": "^7.1.0",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3"
  },
  "dependencies": {
    "@babel/preset-env": "^7.14.7",
    "@swc/core": "^1.2.62",
    "@swc/wasm": "^1.2.62",
    "babel-jest": "^27.0.6",
    "body-parser": "^1.19.0",
    "cookie-session": "^1.4.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.12.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.2",
    "morgan": "^1.10.0",
    "node-notifier": "^10.0.0",
    "nodemon": "^2.0.9",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4",
    "util": "^0.12.4"
  }
